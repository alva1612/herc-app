    "version": "0.2.0",
    "configurations": [
        {
            "name": "test_app",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "args": ["--flavor", "dev"],
            "toolArgs": ["--dart-define-from-file", "./env/local.json"]
        },
        {
            "name": "test_app (dev mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "toolArgs": ["--dart-define-from-file", "/env/dev.json"]
        },
        {
            "name": "staging",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "args": ["--flavor", "staging"],
            "toolArgs": ["--dart-define-from-file", "./env/dev.json"]
        }
    ]
}